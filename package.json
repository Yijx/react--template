{
  "name": "react",
  "version": "0.1.0",
  "private": true,
  "description": "node >= 8.1",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "antd": "^3.23.6",
    "axios": "^0.19.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
